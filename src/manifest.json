{
  "name": "Keyboard Shortcuts for Messenger",
  "version": "2.1.2",
  "description": "Gmail-like keyboard shortcuts for the Facebook Messenger web app (messenger.com).",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://www.messenger.com/*"],
      "js": ["messenger-shortcuts.js"]
    }
  ],
  "permissions": [
    "*://www.messenger.com/*"
  ],
  "icons": { "48": "icon48.png",
             "128": "icon128.png" },
  "homepage_url": "https://github.com/guoguo12/messenger-shortcuts"
}
